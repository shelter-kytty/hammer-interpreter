trans : mat = {
    prepped = map(foldr(_ : x y = x , y ; _) ; mat)

    ret = []

    helper : a = {
        if typeOf(a[1]) == typeOf(0 , 0)
        then {
            ret << map(_ : x = frst x ; a)
            b = map(_ : x = scnd x ; a)
            if typeOf(b[1]) == typeOf(0 , 0)
            then <- helper(b)
            else ret << b
        }
        else ret
    }

    helper(prepped)
}

