
fibNaive : n = {
    if n < 2
    then n
    else fibNaive(n - 1) + fibNaive(n - 2)
}

a1 = clock()

printfn("{0}" ; fibNaive(20))

b1 = clock()

printfn("fibNaive took {0} seconds" ; b1 - a1)



m = [1 1]

fibMemo : n = {
    m[n] or {m << fibMemo(n - 1) + fibMemo(n - 2) ; m[len(m)]}
}

a2 = clock()

printfn("{0}" ; fibMemo(80))

b2 = clock()

printfn("fibMemo took {0} seconds" ; b2 - a2)



fibLog : n = {
    fibInner : a b p q n = {
        if n == 0 then b else {
            if n % 2 == 0 then {
                <- fibInner(
                    a b
                    p * p + q * q
                    2 * p * q + q * q
                    n / 2
                )
            } else {
                <- fibInner(
                    b * q + a * q + a * p
                    b * p + a * q
                    p q
                    n - 1
                )
            }
        }
    }

    fibInner(1 0 0 1 n)
}

a3 = clock()

printfn("{0}" ; fibLog(80))

b3 = clock()

printfn("fibLog took {0} seconds" ; b3 - a3)



fibTail : n = {
    fibInner : n a b = {
        if n == 0 then a else {
            <- fibInner(n - 1 ; b ; a + b)
        }
    }

    <- fibInner(n 0 1)
}

a4 = clock()

printfn("{0}" ; fibTail(80))

b4 = clock()

printfn("fibTail took {0} seconds" ; b4 - a4)



