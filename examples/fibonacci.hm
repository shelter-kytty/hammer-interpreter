fib_Max : n = {
    if n <= 0 then
    return 0
    else unit

    i = n - 1
    f = [[1;0];[0;1]]
    a = [[0;1];[1;1]]

    helper : I F A = {
        if I > 0
        then
            if I % 2 == 0
            then <- helper({I / 2} ; F ; A#A)
            else <- helper({I / 2} ; F#A ; A#A)
        else
            F[2][2]
    }

    helper(i;f;a)
}

a6 = clock()

printfn("{0}"; fib_Max(80))

b6 = clock()

printfn("Max took {0} seconds" ; b6 - a6)
